class Point: Drawable{
    float x = 0.0, y = 0.0;

    function init(float a, float b){
        x = a;
        y = b;
    }
}

class Square: Drawable{
    float x = 0.0, y = 0.0, length = 0.05;

    function init(float a, float b, float len){
        x = a;
        y = b;
        length = len;
    }
}

class FilledSquare: Drawable{
    float x = 0.0, y = 0.0, length = 0.05;

    function init(float a, float b, float len){
        x = a;
        y = b;
        length = len;
    }
}

class Rectangle: Drawable{
    float x = 0.0, y = 0.0, width= 0.05, height = 0.05;

    function init(float a, float b, float w, float h){
        x = a;
        y = b;
        width = w;
        height = h;
    }
}

class FilledRectangle: Drawable{
    float x = 0.0, y = 0.0, width= 0.05, height = 0.05;

    function init(float a, float b, float w, float h){
        x = a;
        y = b;
        width = w;
        height = h;
    }
}

class Circle: Drawable{
    float x = 0.5, y = 0.5, radius = 0.05;

    function init(float a, float b, float rad){
        x = a;
        y = b;
        radius = rad;
    }
}

class FilledCircle: Drawable{
    float x = 0.5, y = 0.5, radius = 0.05;

    function init(float a, float b, float rad){
        x = a;
        y = b;
