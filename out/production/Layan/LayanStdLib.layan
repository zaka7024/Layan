    }
}

class Line: Drawable{
    float a, b, c, d;

    function init(float x, float y, float xx, float yy){
        a = x;
        b = y;
        c = xx;
        d = yy;
    }
}

class Text: Drawable{
    float x = 0.0, y = 0.0;
    int value = "Layan";

    function init(float a, float b, string text){
        x = a;
        y = b;
        value = text;
    }
}

